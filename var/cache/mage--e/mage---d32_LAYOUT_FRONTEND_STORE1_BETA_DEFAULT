            <tmpl>catalog/product/price_msrp_item.phtml</tmpl>
         </action>
      </block>
      <block type="bundle/catalog_product_view_type_bundle" name="product.info.bundle" as="product_type_data" template="bundle/catalog/product/view/type/bundle.phtml"/>
